---
import background from "../assets/background.svg";
---

<div
	class="relative flex flex-col items-center justify-center w-full h-full min-h-screen bg-gray-100"
>
	<img
		src={background.src}
		alt="Background"
		class="absolute inset-0 object-cover w-full h-full"
		fetchpriority="high"
	/>
</div>
